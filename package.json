{
  "name": "cordova-plugin-app-name-letsworks",
  "version": "1.0.7",
  "description": "A Cordova/PhoneGap plugin that allows you to give you app a name with spaces and special characters and stuff.",
  "cordova": {
    "id": "cordova-plugin-app-name-letsworks",
    "platforms": [
      "ios",
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/queenyjina/cordova-plugin-app-name-letsworks.git"
  },
  "bugs": {
    "url": "https://github.com/queenyjina/cordova-plugin-app-name-letsworks/issues"
  },
  "homepage": "https://github.com/queenyjina/cordova-plugin-app-name-letsworks",
  "keywords": [
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova:plugin",
    "spaces",
    "app",
    "name",
    "rename"
  ],
  "license": "MIT",
  "dependencies": {
    "semver": "^6.3.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "libxmljs": "^0.18.4"
  },
  "scripts": {
    "postversion": "node scripts/sync-plugin-version.js; git add plugin.xml; git commit plugin.xml -m \"v${npm_package_version}\""
  }
}
